{% extends 'base.html.twig' %}

{% block title %}SeedType index{% endblock %}

{% block body %}
    <h1>SeedType index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for seed_type in seed_types %}
            <tr>
                <td>{{ seed_type.id }}</td>
                <td>{{ seed_type.name }}</td>
                <td>
                    <a href="{{ path('app_seed_type_show', {'id': seed_type.id}) }}">show</a>
                    <a href="{{ path('app_seed_type_edit', {'id': seed_type.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_seed_type_new') }}">Create new</a>
{% endblock %}
